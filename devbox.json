{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "nodejs_22@latest",
    "corepack@latest",
    "coreutils@latest",
    "act@latest",
    "docker@latest",
    "toml-cli@latest",
    "supabase-cli@latest"
  ],
  "env": {},
  "shell": {
    "init_hook": ["export PATH=$(pwd)/node_modules/.bin:$PATH"],
    "scripts": {
      "prepare": [
        "corepack install",
        "alias pnpm='corepack pnpm'",
        "alias pnpx='corepack pnpx'",
        "pnpm install --force",
        "pnpx supabase start",
        "pnpm types",
        "pnpm format",
        "pnpm lint",
        "pnpm build",
        "pnpm test"
      ],
      "dev": ["pnpx inngest-cli@latest dev & pnpm dev"],
      "test": ["act push -j test"]
    }
  }
}
